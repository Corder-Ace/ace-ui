{
	"name": "ace-components",
	"version": "1.0.0",
	"private": true,
	"description": "wechat",
	"main": "lib/index.js",
	"module": "es/index.js",
	"typings": "lib/index.d.ts",
	"files": [
		"dist",
		"lib",
		"es"
	],
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"build:es": "babel src/components -d es -s",
		"build:ui": "webpack --config build/webpack.prod.config.js",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"build:component": "cross-env TARO_BUILD_TYPE=component taro build --ui --ui-index=./src/index.js",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "Ace",
	"dependencies": {
		"@babel/runtime": "^7.10.2",
		"@tarojs/components": "3.0.0-rc.3",
		"@tarojs/react": "3.0.0-rc.3",
		"@tarojs/runtime": "3.0.0-rc.3",
		"@tarojs/taro": "3.0.0-rc.3",
		"classnames": "^2.2.6",
		"react": "^16.10.0",
		"react-dom": "^16.10.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.1",
		"@babel/core": "^7.10.2",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.1",
		"@babel/plugin-transform-object-assign": "^7.10.1",
		"@babel/plugin-transform-runtime": "^7.10.1",
		"@babel/preset-env": "^7.10.2",
		"@babel/preset-react": "^7.10.1",
		"@babel/preset-typescript": "^7.10.1",
		"@tarojs/mini-runner": "3.0.0-rc.3",
		"@tarojs/webpack-runner": "3.0.0-rc.3",
		"@types/react": "^16.0.0",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^2.x",
		"@typescript-eslint/parser": "^2.x",
		"babel-loader": "^8.1.0",
		"babel-plugin-import": "^1.13.0",
		"babel-preset-taro": "3.0.0-rc.3",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.0.0-rc.3",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^1.6.1",
		"favicons-webpack-plugin": "^3.0.1",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"postcss-cssnext": "^3.1.0",
		"postcss-flexbugs-fixes": "^4.2.1",
		"postcss-nested": "^4.2.1",
		"postcss-preset-env": "^6.7.0",
		"sass-loader": "^8.0.2",
		"sinon_cli": "^1.0.4",
		"stylelint": "9.3.0",
		"typescript": "^3.7.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	}
}
